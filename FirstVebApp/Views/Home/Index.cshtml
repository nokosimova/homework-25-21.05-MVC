@model List<FirstVebApp.Models.Person>  
@{
    //ViewData["Title"] = "Список покупателей: ";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Списки</title>
</head>
<body>
    <h3>Покупатели</h3>
    <table>
        <tr>
            <td>id:</td>
            <td>Фамилия:</td>
            <td>Имя:</td>
            <td>Отчество:</td>
            <td></td>
        </tr>
        @foreach (var person in Model)
        {
        <tr>
            <td>@person.id</td>
            <td>@person.LastName</td>
            <td>@person.FirstName</td>
            <td>@person.MiddleName</td>
        </tr>
        }
    </table>
</body>
</html>